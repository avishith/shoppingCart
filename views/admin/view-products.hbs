<section>
 <div class="container">
  
    
     <admin></admin>
   </div>
 </div>
  <div class="container">
     <div class="row mt-4">
        <a href="/admin/add-products" class="btn btn-success ml-auto">ADD Products
        </a>
     </div>
    <table class="table mt-5">
      <thead>
        <tr>
          <th scope="col">no</th>
          <th scope="col">title</th>
          <th scope="col">Category</th>
          <th scope="col">description</th>
          <th scope="col">images</th>
        </tr>
      </thead>
      <tbody>
        {{#each products}}

        <tr>
          <th scope="row">1</th>
          <td>{{this.name}}</td>
          <td>{{this.category}}</td>
          <td>{{this.description}}</td>
          <td><img style="width: 50px; height:50px;" src="{{this.image}}"alt="photo"></td>
        </tr>
        {{/each}}



      </tbody>
    </table>
  </div>
</section>